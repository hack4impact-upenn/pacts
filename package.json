{
    "name": "PACTS",
    "version": "1.0.0",
    "author": "Hack4Impact",
    "license": "MIT",
    "description": "PACTS Project",
    "main": "server/server.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/duhaime/react-boilerplate.git"
    },
    "scripts": {
        "start": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --port 7081 --history-api-fallback",
        "build": "./node_modules/webpack/bin/webpack.js --config webpack.config.js",
        "compress": "./node_modules/webpack/bin/webpack.js --config webpack.config.js",
        "development": "npm run start && npm run serve",
        "production": "npm run build && npm run serve",
        "serve": "FLASK_APP=server/app.py flask run --port=7082",
        "lint-js": "./node_modules/eslint/bin/eslint.js -c './src/config/.eslintrc.js' src/",
        "lint-py": "pylint server/app.py",
        "lint": "npm run lint-js && npm run lint-py",
        "jest": "./node_modules/jest/bin/jest.js",
        "test": "npm run build && npm run lint && npm run jest && pytest"
    },
    "dependencies": {
        "babel-polyfill": "^6.26.0",
        "connected-react-router": "^6.4.0",
        "history": "^4.9.0",
        "isomorphic-fetch": "^2.2.1",
        "lodash": "^4.17.11",
        "react": "^16.8.6",
        "react-bootstrap": "^1.0.0-beta.8",
        "react-dom": "^16.8.6",
        "react-redux": "^7.0.2",
        "react-router": "^5.0.0",
        "react-router-dom": "^5.0.0",
        "redux": "^4.0.1",
        "redux-form": "^8.2.0",
        "redux-freeze": "^0.1.7",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.3.0",
        "webpack-cli": "^3.3.1"
    },
    "devDependencies": {
        "babel-core": "^6.26.3",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^24.7.1",
        "babel-loader": "^8.0.5",
        "babel-preset-env": "^1.7.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-react-hmre": "^1.1.1",
        "clean-webpack-plugin": "^2.0.1",
        "compression-webpack-plugin": "^2.0.0",
        "copy-webpack-plugin": "^5.0.3",
        "css-loader": "^2.1.1",
        "eslint": "^5.16.0",
        "eslint-plugin-react": "^7.12.4",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^3.0.1",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^24.7.1",
        "json-loader": "^0.5.7",
        "jsx-loader": "^0.13.2",
        "optimize-css-assets-webpack-plugin": "^5.0.1",
        "prop-types": "^15.7.2",
        "react-test-renderer": "^16.8.6",
        "style-loader": "^0.23.1",
        "webpack": "^4.30.0",
        "webpack-dev-server": "^3.3.1",
        "webpack-merge": "^4.2.1"
    },
    "babel": {
        "presets": [
            "env",
            "react"
        ]
    }
}
